#include <stdio.h>
int main()
{
    int i , a[999999] , n ;
    printf("How many numbers you want to enter : ");
    scanf("%d", &n);
    printf("\nEnter the elements : ");
    for (i=0 ; i<=n-1 ; i++)
    {
        scanf("%d", &a[i]);
    }
    printf("\nReversed values are ");
    for (i=n-1 ; i>=0 ; i--)
    {
        printf("%d ", a[i]);
    }
    return 0;
}
